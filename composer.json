{
    "name": "dflydev/encrypted-fig-cookies",
    "description": "Encrypted Cookies for PSR-7 HTTP Message Interface.",
    "license": "MIT",
    "keywords": ["psr7", "psr-7", "cookies"],
    "authors": [
        {
            "name": "Beau Simensen",
            "email": "beau@dflydev.com"
        }
    ],
    "require": {
        "dflydev/fig-cookies": "^1.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dflydev\\EncryptedFigCookies\\": "src/Dflydev/EncryptedFigCookies"
        }
    },
    "require-dev": {
        "phpseclib/phpseclib": "^0.3.10",
        "zendframework/zend-crypt": "^2.0",
        "zendframework/zend-filter": "^2.0",
        "defuse/php-encryption": "^1.2",

        "codeclimate/php-test-reporter": "~0.1@dev",
        "phpunit/phpunit": "^4.6",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Dflydev\\EncryptedFigCookies\\": "tests/Dflydev/EncryptedFigCookies"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
